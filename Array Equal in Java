

package SomePrograms;
public class ArrayEqual {
	// check whether two array are equal
	public static boolean arrayChecker(int a[], int b[]) {
		if (a.length != b.length) {
			return false;

		}
		boolean data = true;
		for (int i = 0; i < a.length; i++) {
			if (a[i] != b[i]) {
				data = false;
			}
		}
		return data;
	}

	public static void main(String[] args) {
		int a[] = { 1, 2, 3, 4, 5 };
		int b[] = { 1, 2, 3, 4, 5 };
		System.out.println(arrayChecker(a, b));
	}
}


